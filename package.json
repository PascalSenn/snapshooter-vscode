{
  "name": "snapshooter-vscode",
  "publisher": "pascalsenn",
  "bugs": {
    "url": "https://github.com/PascalSenn/snapshooter-vscode/issues"
  },
  "repository": {
    "url": "https://github.com/PascalSenn/snapshooter-vscode",
    "type": "git"
  },
  "displayName": "Snapshooter",
  "description": "Provides Code Lens for Snapshooter",
  "author": {
    "name": "Pascal Senn",
    "url": "https://twitter.com/Pascal_Senn"
  },
  "version": "0.0.3",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.64.0"
  },
  "keywords": [
    "snapshooter",
    "csharp",
    ".net",
    "dotnet",
    "testing",
    "snapshot"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:csharp"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.64.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4",
    "ts-loader": "^9.2.6",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "@vscode/test-electron": "^2.0.3"
  }
}
